{
  "name": "scribi",
  "version": "0.1.0",
  "description": "A developer-first WYSIWYG editor for React - feature-rich, customizable, and framework-agnostic",
  "type": "module",
  "license": "MIT",
  "author": "Your Name",
  "homepage": "https://github.com/yourusername/scribi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/scribi.git"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "rich-text",
    "tiptap",
    "react",
    "typescript",
    "markdown",
    "notion",
    "developer-tools"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "rstest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@tiptap/core": "^2.11.2",
    "@tiptap/extension-character-count": "^2.11.2",
    "@tiptap/extension-code-block-lowlight": "^2.11.2",
    "@tiptap/extension-color": "^2.11.2",
    "@tiptap/extension-highlight": "^2.11.2",
    "@tiptap/extension-horizontal-rule": "^2.11.2",
    "@tiptap/extension-image": "^2.11.2",
    "@tiptap/extension-link": "^2.11.2",
    "@tiptap/extension-placeholder": "^2.11.2",
    "@tiptap/extension-task-item": "^2.11.2",
    "@tiptap/extension-task-list": "^2.11.2",
    "@tiptap/extension-text-style": "^2.11.2",
    "@tiptap/extension-underline": "^2.11.2",
    "@tiptap/extension-youtube": "^2.11.2",
    "@tiptap/pm": "^2.11.2",
    "@tiptap/react": "^2.11.2",
    "@tiptap/starter-kit": "^2.11.2",
    "@tiptap/suggestion": "^2.11.2",
    "cmdk": "^1.0.4",
    "jotai": "^2.11.0",
    "lowlight": "^3.1.0",
    "tippy.js": "^6.3.7",
    "tiptap-markdown": "^0.8.10",
    "tunnel-rat": "^0.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.15.0",
    "@rstest/core": "^0.5.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.10.6",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.5",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "private": false
}
